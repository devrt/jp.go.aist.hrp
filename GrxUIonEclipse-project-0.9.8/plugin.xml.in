<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         id="applications"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run class="com.generalrobotix.ui.grxui.Application"/>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.generalrobotix.ui.grxui.GrxUIPerspectiveFactory"
            icon="resources/images/grxrobot.png"
            id="com.generalrobotix.ui.grxui.GrxUIPerspectiveFactory"
            name="GrxUI">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="com.generalrobotix.ui.view"
            name="GrxUI"/>
      <view
            class="com.generalrobotix.ui.view.GrxItemViewPart"
            id="com.generalrobotix.ui.view.GrxItemViewPart"
            category="com.generalrobotix.ui.view"
            name="Item View">
      </view>
      @GRX_PATH_PLANNING_VIEW_PART@
      <view
            class="com.generalrobotix.ui.view.GrxRangeSensorViewPart"
            id="com.generalrobotix.ui.view.GrxRangeSensorViewPart"
            category="com.generalrobotix.ui.view"
            name="Range Sensor View">
      </view>
      <view
            class="com.generalrobotix.ui.view.GrxJythonPromptViewPart"
            id="com.generalrobotix.ui.view.GrxJythonPromptViewPart"
            category="com.generalrobotix.ui.view"
            name="JythonPrompt View"/>
      <view
            class="com.generalrobotix.ui.view.GrxPropertyViewPart"
            id="com.generalrobotix.ui.view.GrxPropertyViewPart"
            category="com.generalrobotix.ui.view"
            name="Property View"/>
      <view
            class="com.generalrobotix.ui.view.GrxORBMonitorViewPart"
            id="com.generalrobotix.ui.view.GrxORBMonitorViewPart"
            category="com.generalrobotix.ui.view"
            name="NameService Monitor"/>
      <view
            class="com.generalrobotix.ui.view.Grx3DViewPart"
            id="com.generalrobotix.ui.view.Grx3DViewPart"
            category="com.generalrobotix.ui.view"
            name="3DView"/>
      <view
            class="com.generalrobotix.ui.view.GrxLoggerViewPart"
            id="com.generalrobotix.ui.view.GrxLoggerViewPart"
            category="com.generalrobotix.ui.view"
            name="Logger View"/>
      <view
            class="com.generalrobotix.ui.view.GrxGraphViewPart"
            id="com.generalrobotix.ui.view.GrxGraphViewPart"
            category="com.generalrobotix.ui.view"
            name="Graph"/>
      <view
            class="com.generalrobotix.ui.view.GrxProcessManagerViewPart"
            id="com.generalrobotix.ui.view.GrxProcessManagerViewPart"
            category="com.generalrobotix.ui.view"
            name="Process Manager"/>
      <view
            class="com.generalrobotix.ui.view.GrxOpenHRPViewPart"
            id="com.generalrobotix.ui.view.GrxOpenHRPViewPart"
            category="com.generalrobotix.ui.view"
            name="OpenHRP"/>
      <view
            class="com.generalrobotix.ui.view.GrxRobotStatViewPart"
            id="com.generalrobotix.ui.view.GrxRobotStatViewPart"
            category="com.generalrobotix.ui.view"
            name="Robot State"/>
      <view
            class="com.generalrobotix.ui.view.GrxTextEditorViewPart"
            id="com.generalrobotix.ui.view.GrxTextEditorViewPart"
            category="com.generalrobotix.ui.view"
            name="Text Editor">
      </view>
      <view
            class="com.generalrobotix.ui.view.GrxServerManagerViewPart"
            id="com.generalrobotix.ui.view.GrxServerManagerViewPart"
            category="com.generalrobotix.ui.view"
            name="Server Manager"/>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="com.generalrobotix.ui.actionSet"
            label="GrxUI Actions"
            visible="false">
         <menu
               id="GrxUIMenu"
               label="&amp;GrxUI">
            <separator
                  name="GrxUIGroup">
            </separator>
         </menu>
         <action
               class="com.generalrobotix.ui.actions.StartSimulate"
               icon="resources/images/sim_start.png"
               id="com.generalrobotix.ui.actions.StartSimulate"
               label="&amp;Start Simulation"
               menubarPath="GrxUIMenu/GrxUIGroup"
               toolbarPath="GrxUIGroup"
               tooltip="Start Simulation">
         </action>
         <action
               class="com.generalrobotix.ui.actions.SaveProjectAs"
               id="com.generalrobotix.ui.actions.SaveProjectAs"
               label="Save Project As"
               menubarPath="GrxUIMenu/GrxUIGroup">
         </action>
         <action
               class="com.generalrobotix.ui.actions.SaveProject"
               id="com.generalrobotix.ui.actions.SaveProject"
               label="Save Project"
               menubarPath="GrxUIMenu/GrxUIGroup">
         </action>
         <action
               class="com.generalrobotix.ui.actions.RestoreProject"
               id="com.generalrobotix.ui.actions.RestoreProject"
               label="Restore Project"
               menubarPath="GrxUIMenu/GrxUIGroup"
               style="push">
         </action>
         <action
               class="com.generalrobotix.ui.actions.LoadProject"
               id="com.generalrobotix.ui.actions.LoadProject"
               label="Load Project"
               menubarPath="GrxUIMenu/GrxUIGroup">
         </action>
         <action
               class="com.generalrobotix.ui.actions.NewProject"
               id="com.generalrobotix.ui.actions.NewProject"
               label="New Project"
               menubarPath="GrxUIMenu/GrxUIGroup">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="com.generalrobotix.ui.grxui.GrxUIPerspectiveFactory">
         <actionSet
               id="com.generalrobotix.ui.actionSet"
               label="GrxUI Actions"
               visible="true">
         </actionSet>
      </perspectiveExtension>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.generalrobotix.ui.grxui.applications"
            name="GRXUI"/>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.generalrobotix.ui.grxui.GrxuiPreferencePage"
            id="com.generalrobotix.ui.grxui.GrxuiPreferencePage"
            name="GRXUI"/>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer class="com.generalrobotix.ui.grxui.PreferenceInitializer"/>
   </extension>

</plugin>
