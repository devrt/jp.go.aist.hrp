<?xml version="1.0" encoding="UTF-8"?>
<grxui>
    <processmanagerconfig>
        <process dir="$(BIN_DIR)" id="" waitcount="500"/>
        <process args="-start 2809 -logdir $(TEMP)" autostart="true" com="$(OMNI_ROOT)/bin/x86_win32/omniNames" id="NameService"/>
        <process args="-ORBInitRef NameService=corbaloc:iiop:localhost:2809/NameService" autostart="true" com="$(BIN_DIR)/openhrp-model-loader" hasshutdown="true" id="ModelLoader"/>
        <process args="-ORBInitRef NameService=corbaloc:iiop:localhost:2809/NameService" autostart="true" com="$(BIN_DIR)/openhrp-collision-detector" hasshutdown="true" id="CollisionDetectorFactory"/>
        <process args="-ORBInitRef NameService=corbaloc:iiop:localhost:2809/NameService" autostart="true" com="$(BIN_DIR)/openhrp-aist-dynamics-simulator" hasshutdown="true" id="DynamicsSimulatorFactory" waitcount="1000"/>
    </processmanagerconfig>

    <mode name="Simulation" select="true">
        <item class="com.generalrobotix.ui.item.GrxModelItem" visible="true"/>
        <item class="com.generalrobotix.ui.item.GrxLinkItem" visible="false"/>
        <item class="com.generalrobotix.ui.item.GrxShapeItem" visible="false"/>
        <item class="com.generalrobotix.ui.item.GrxSensorItem" visible="false"/>
        <item class="com.generalrobotix.ui.item.GrxWorldStateItem" visible="true"/>
        <item class="com.generalrobotix.ui.item.GrxCollisionPairItem" visible="true"/>
        <item class="com.generalrobotix.ui.item.GrxGraphItem" visible="true"/>
        <item class="com.generalrobotix.ui.item.GrxPythonScriptItem" visible="true"/>
    </mode>
</grxui>
